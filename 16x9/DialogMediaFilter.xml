<?xml version="1.0" encoding="UTF-8"?>
<!-- Foundation -->
<window>
	<defaultcontrol always="true">5</defaultcontrol>
	<allowoverlay>yes</allowoverlay>
	<controls>
		<control type="group">
			<animation effect="fade" start="100" end="0" time="400" condition="Window.IsVisible(SliderDialog) | Window.IsVisible(FileBrowser)">Conditional</animation>
			<include>GlobalOverlay</include>
		</control>
		<control type="group">
			<animation effect="fade" start="100" end="0" time="400" condition="Window.IsVisible(SliderDialog) | Window.IsVisible(FileBrowser)">Conditional</animation>
			<include>DefDialogInfoPanel</include>
			<control type="label" id="1">
				<description>Header Label</description>
				<centerleft>50%</centerleft>
				<posy>15</posy>
				<width>990</width>
				<align>center</align>
				<label>5</label>
				<textcolor>$VAR[EmbyMenuColourHighlight]</textcolor>
				<font>settingsheader</font>
			</control>
			<control type="grouplist" id="5">
				<description>Control Area</description>
				<left>0</left>
				<right>0</right>
				<top>92</top>
				<height>640</height>
				<itemgap>0</itemgap>
				<orientation>vertical</orientation>
				<pagecontrol>60</pagecontrol>
				<onleft>28</onleft>
				<onright>28</onright>
				<onup>28</onup>
				<ondown>28</ondown>
			</control>
			<control type="button" id="28">
				<description>OK</description>
				<centerleft>33%</centerleft>
				<bottom>33</bottom>
				<width>270</width>
				<label>186</label>
				<onleft>27</onleft>
				<onright>27</onright>
				<onup>5</onup>
				<ondown>5</ondown>
				<include>DefDialogButtons</include>
			</control>
			<control type="button" id="27">
				<description>Defaults</description>
				<centerleft>66%</centerleft>
				<bottom>33</bottom>
				<width>270</width>
				<label>192</label>
				<onleft>28</onleft>
				<onright>28</onright>
				<onup>5</onup>
				<ondown>5</ondown>
				<include>DefDialogButtons</include>
			</control>
			<control type="button" id="7">
				<description>Default Button</description>
				<align>left</align>
				<texturefocus colordiffuse="$VAR[ColourHighlight]">common/box.png</texturefocus>
			</control>
			<control type="radiobutton" id="8">
				<description>Default RadioButton</description>
				<align>left</align>
				<radioposx>1230</radioposx>
				<texturefocus colordiffuse="$VAR[ColourHighlight]">common/box.png</texturefocus>
			</control>
			<control type="spincontrolex" id="9">
				<description>Default spincontrolex</description>
				<align>left</align>
				<reverse>yes</reverse>
				<texturefocus colordiffuse="$VAR[ColourHighlight]">common/box.png</texturefocus>
			</control>
			<control type="button" id="12">
				<description>Default Button</description>
				<align>left</align>
				<texturefocus colordiffuse="$VAR[ColourHighlight]">common/box.png</texturefocus>
			</control>
			<control type="sliderex" id="13">
				<description>Default Slider</description>
				<texturefocus colordiffuse="$VAR[ColourHighlight]">common/box.png</texturefocus>
			</control>
			<control type="image">
				<posx>660</posx>
				<top>92</top>
				<height>640</height>
				<width>1</width>
				<texture colordiffuse="white">common/white.png</texture>
			</control>
		</control>
	</controls>
</window>