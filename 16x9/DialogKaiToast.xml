<?xml version="1.0" encoding="UTF-8"?>
<!-- Foundation -->
<window id="107">
	<allowoverlay>no</allowoverlay>
	<controls>
		<control type="group">
			<right>0</right>
			<posy>25</posy>
			<width>500</width>
			<height>74</height>
		<animation type="Visible" reversible="false">
			<effect type="slide" start="500" end="0" center="auto" tween="back" easing="out" time="450" delay="150" />
		</animation>
		<animation type="WindowOpen" reversible="false">
			<effect type="slide" start="500" end="0" center="auto" tween="back" easing="out" time="450" delay="150" />
		</animation>
		<animation effect="fade" end="0" start="100" time="150">Hidden</animation>
		<animation effect="fade" end="0" start="100" time="150">WindowClose</animation>
			<control type="image">
				<description>background image</description>
				<width>500</width>
				<height>74</height>
				<animation effect="fade" start="100" end="90" condition="[!Window.IsVisible(fullscreenvideo) + !Window.IsVisible(visualisation)]">Conditional</animation>
				<texture colordiffuse="$VAR[EmbyMenuColourHighlight]">common/box.png</texture>
			</control>
			<control type="group">
				<control type="label">
					<left>20</left>
					<centertop>50%</centertop>
					<width>64</width>
					<height>64</height>
					<aligny>center</aligny>
					<align>center</align>
					<font>symbol</font>
					<label></label>
					<textcolor>$VAR[EmbyMenuTextColourHighlight]</textcolor>
					<visible>SubString(Control.GetLabel(401),error)</visible>
				</control>
				<control type="label">
					<left>20</left>
					<centertop>50%</centertop>
					<width>64</width>
					<height>64</height>
					<aligny>center</aligny>
					<align>center</align>
					<font>symbol</font>
					<label></label>
					<textcolor>$VAR[EmbyMenuTextColourHighlight]</textcolor>
					<visible>SubString(Control.GetLabel(400),info.png)</visible>
				</control>
				<control type="label">
					<left>20</left>
					<centertop>50%</centertop>
					<width>64</width>
					<height>64</height>
					<aligny>center</aligny>
					<align>center</align>
					<font>symbol</font>
					<label></label>
					<textcolor>$VAR[EmbyMenuTextColourHighlight]</textcolor>
					<visible>SubString(Control.GetLabel(400),warnning.png)</visible>
				</control>
				<control type="image" id="400">
					<left>20</left>
					<centertop>50%</centertop>
					<width>64</width>
					<height>64</height>
					<aspectratio>keep</aspectratio>
				</control>
				<control type="label" id="401">
					<left>104</left>
					<width>auto</width>
					<top>12</top>
					<height>24</height>
					<aligny>center</aligny>
					<font>TinyBold</font>
					<textcolor>$VAR[EmbyMenuTextColourHighlight]</textcolor>
				</control>
				<control type="label" id="402">
					<left>104</left>
					<width>auto</width>
					<bottom>12</bottom>
					<height>24</height>
					<aligny>center</aligny>
					<font>Tiny</font>
					<textcolor>$VAR[EmbyMenuTextColourHighlight]</textcolor>
				</control>
			</control>
		</control>
	</controls>
