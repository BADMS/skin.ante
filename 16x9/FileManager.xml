<?xml version="1.0" encoding="UTF-8"?>
<!-- Foundation -->
<window id="3">
	<defaultcontrol always="true">20</defaultcontrol>
	<allowoverlay>yes</allowoverlay>
	<controls>
		<include condition="!Skin.HasSetting(lights.out)">GlobalBackground</include>
		<include>Furniture_NowPlaying</include>
		<include>Furniture_HeaderEmby</include>
		<include>Furniture_EmbyClock</include>
		<control type="group">
			<include condition="!Skin.HasSetting(low.power)">Animation.Common</include><include condition="Skin.HasSetting(low.power)">Animation.Common.Lowpower</include>
			<control type="group">
				<description>Left panel</description>
				<left>NewSidePad</left>
				<top>TopPad</top>
				<height>901</height>
				<width>945</width>
				<control type="image">
					<width>100%</width>
					<height>73</height>
					<texture colordiffuse="$VAR[EmbyMenuTextColourHighlight]">common/box.png</texture>
				</control>
				<control type="label" id="101">
					<description>Header Label</description>
					<top>5</top>
					<left>30</left>
					<right>30</right>
					<align>left</align>
					<font>SmallHeaderFont</font>
					<textcolor>$VAR[EmbyMenuColourHighlight]</textcolor>
				</control>
				<control type="list" id="20">
					<description>Left File List</description>
					<top>73</top>
					<height>828</height>
					<onleft>21</onleft>
					<onright>21</onright>
					<onup>20</onup>
					<ondown>20</ondown>
					<itemlayout height="69" width="945">
						<control type="image">
							<top>1</top>
							<height>67</height>
							<texture colordiffuse="$VAR[EmbyMenuColourHighlight]">common/box.png</texture>
						</control>
						<control type="label">
							<left>10</left>
							<right>10</right>
							<align>left</align>
							<textcolor>$VAR[EmbyMenuTextColourHighlight]</textcolor>
							<info>ListItem.Label</info>
						</control>
						<control type="label">
							<left>10</left>
							<right>10</right>
							<align>right</align>
							<font>Tiny</font>
							<textcolor>$VAR[EmbyMenuTextColourHighlight]</textcolor>
							<info>ListItem.Label2</info>
						</control>
					</itemlayout>
					<focusedlayout height="69" width="945">
						<control type="image">
							<width>100%</width>
							<height>66</height>
							<texture colordiffuse="$VAR[EmbyLeftColourHighlight]">common/box.png</texture>
							<animation effect="fade" start="100" end="50" condition="!Control.HasFocus(20)">Conditional</animation>
						</control>
						<control type="label">
							<left>10</left>
							<right>10</right>
							<align>left</align>
							<textcolor>$VAR[ColourSelected]</textcolor>
							<info>ListItem.Label</info>
						</control>
						<control type="label">
							<left>10</left>
							<right>10</right>
							<align>right</align>
							<textcolor>$VAR[ColourSelected]</textcolor>
							<font>Tiny</font>
							<info>ListItem.Label2</info>
						</control>
					</focusedlayout>
				</control>
			</control>
			<control type="group">
				<description>Right panel</description>
				<right>NewSidePad</right>
				<top>TopPad</top>
				<height>901</height>
				<width>945</width>
				<control type="image">
					<width>100%</width>
					<height>73</height>
					<texture colordiffuse="$VAR[EmbyMenuTextColourHighlight]">common/box.png</texture>
				</control>
				<control type="label" id="102">
					<description>Header Label</description>
					<top>5</top>
					<left>30</left>
					<right>30</right>
					<align>left</align>
					<font>SmallHeaderFont</font>
					<textcolor>$VAR[EmbyMenuColourHighlight]</textcolor>
				</control>
				<control type="list" id="21">
					<description>Left File List</description>
					<top>73</top>
					<height>828</height>
					<onleft>20</onleft>
					<onright>20</onright>
					<onup>21</onup>
					<ondown>21</ondown>
					<itemlayout height="69" width="945">
						<control type="image">
							<top>1</top>
							<height>67</height>
							<texture colordiffuse="$VAR[EmbyMenuColourHighlight]">common/box.png</texture>
						</control>
						<control type="label">
							<left>10</left>
							<right>10</right>
							<align>left</align>
							<textcolor>$VAR[EmbyMenuTextColourHighlight]</textcolor>
							<info>ListItem.Label</info>
						</control>
						<control type="label">
							<left>10</left>
							<right>10</right>
							<align>right</align>
							<font>Tiny</font>
							<textcolor>$VAR[EmbyMenuTextColourHighlight]</textcolor>
							<info>ListItem.Label2</info>
						</control>
					</itemlayout>
					<focusedlayout height="69" width="945">
						<control type="image">
							<width>100%</width>
							<height>66</height>
							<texture colordiffuse="$VAR[EmbyRightColourHighlight]">common/box.png</texture>
							<animation effect="fade" start="100" end="50" condition="!Control.HasFocus(21)">Conditional</animation>
						</control>
						<control type="label">
							<left>10</left>
							<right>10</right>
							<align>left</align>
							<textcolor>$VAR[ColourSelected]</textcolor>
							<info>ListItem.Label</info>
						</control>
						<control type="label">
							<left>10</left>
							<right>10</right>
							<align>right</align>
							<textcolor>$VAR[ColourSelected]</textcolor>
							<font>Tiny</font>
							<info>ListItem.Label2</info>
						</control>
					</focusedlayout>
				</control>
			</control>
		</control>
	</controls>
</window>